#!/usr/bin/env bash

#@@ common: @@
#@@ include-if: SELFUPDATE disable helper/update-disabled.sh helper/update.sh @@
#@@ var: SELFUPDATE_DISABLED_MSG @@

VERSION="4.1.0-dev"

case "$1" in
  -v|--version)
    echo $VERSION && exit 0
    ;;
  update)
    update
    ;;
  *)
    git extras --help
    ;;
esac
