#!/bin/sh

source git_bug_feature_refactor


if test -z $1; then
  # no feature specified, list the features
  list_branch_type refactor
  
elif contains 'finish fin end complete' $1; then
  finish_refactor_branch $2

elif contains 'delete del remove rm clear' $1; then
  delete_refactor_branch $2
  
else
  switch_refactor_branch $1
fi
